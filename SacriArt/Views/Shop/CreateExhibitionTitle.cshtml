@model SacriArt.Models.ShopModels.ExhibitionTitle

@{
    
}
<h2>Adding new Exhibition Title</h2>

<div class="container">
    <form asp-action="CreateExhibitionTitle" asp-controller="Shop" method="post" enctype="multipart/form-data">
        
        <p>
            @Html.LabelFor(m => m.Name, "Exhibition Title")
            @Html.EditorFor(m => m.Name)
        </p>
       
           
        <p>
            <input type="submit" value="Save" />
        </p>
    </form>
</div>

<script>
    $(".select option").each(function () {
        $(this).siblings('[value="' + this.value + '"]').remove();
    });
</script>
        